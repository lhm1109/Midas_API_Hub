{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "bCOMB_SHEAR_TORS": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Combined Shear and Torsion Check",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bCRACK_CHK": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Crack Check",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bFLEX_STR_CHK": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Flexural Strength Check",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bPRINC_CONSTR": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Construction Stage",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bPRINC_MAX_SHEAR": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Service Load (Max Shear)",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bPRINC_MAX_TORS": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Service Load (Max Torsion)",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bSHEAR_STR_CHK": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Shear Strength Check",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bSTRS_CONSTR": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress by Construction Stage",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bSTRS_PREST": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress in Prestressing Tendons",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "bSTRS_SVC_COMB": {
      "default": false,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress by Service Load Combinations",
        "sectionId": "SECTION_OUTPUT"
      }
    },
    "dEXP_VAL": {
      "default": 1,
      "description": "User가 직접 입력하는 Exposure Factor 값",
      "type": "number",
      "x-required-when": {
        "equals": 2,
        "field": "iEXP_FACT"
      },
      "x-ui": {
        "label": "User Exposure Factor Value",
        "sectionId": "SECTION_INPUT"
      }
    },
    "iCONSTR_TYPE": {
      "default": 1,
      "oneOf": [
        {
          "const": 0,
          "title": "Segmental"
        },
        {
          "const": 1,
          "title": "Non-Segmental"
        }
      ],
      "type": "integer",
      "x-ui": {
        "label": "Construction Type",
        "sectionId": "SECTION_INPUT"
      }
    },
    "iCORR_COND": {
      "default": 0,
      "oneOf": [
        {
          "const": 0,
          "title": "Severe"
        },
        {
          "const": 1,
          "title": "Moderate/Mild"
        }
      ],
      "type": "integer",
      "x-ui": {
        "label": "Corrosive Condition",
        "sectionId": "SECTION_INPUT"
      }
    },
    "iEXP_FACT": {
      "default": 0,
      "oneOf": [
        {
          "const": 0,
          "title": "Class I (1.0)"
        },
        {
          "const": 1,
          "title": "Class II (0.75)"
        },
        {
          "const": 2,
          "title": "User"
        }
      ],
      "type": "integer",
      "x-ui": {
        "label": "Exposure Factor for Crack Width",
        "sectionId": "SECTION_INPUT"
      }
    },
    "iFLEX_STR": {
      "default": 0,
      "oneOf": [
        {
          "const": 0,
          "title": "Code"
        },
        {
          "const": 1,
          "title": "Strain Compatibility"
        }
      ],
      "type": "integer",
      "x-ui": {
        "label": "Flexural Strength",
        "sectionId": "SECTION_INPUT"
      }
    },
    "iTEND_TYPE": {
      "default": 0,
      "oneOf": [
        {
          "const": 0,
          "title": "Low Relaxation Tendons"
        },
        {
          "const": 1,
          "title": "Stress Relieved Tendons"
        },
        {
          "const": 2,
          "title": "Prestressing Bars"
        }
      ],
      "type": "integer",
      "x-ui": {
        "label": "Tendon Type",
        "sectionId": "SECTION_INPUT"
      }
    },
    "sDES_CODE": {
      "default": "AASHTO-LRFD20",
      "enum": [
        "AASHTO-LRFD20",
        "AASHTO-LRFD17",
        "AASHTO-LRFD16",
        "AASHTO-LRFD14",
        "AASHTO-LRFD12",
        "AASHTO-LRFD07"
      ],
      "type": "string",
      "x-ui": {
        "label": "Design Code",
        "sectionId": "SECTION_GENERAL"
      }
    }
  },
  "required": [
    "sDES_CODE",
    "iTEND_TYPE"
  ],
  "title": "PSC Design Code",
  "type": "object"
}
