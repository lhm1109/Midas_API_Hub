{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Displacements",
  "type": "object",
  "required": [
    "Argument"
  ],
  "additionalProperties": false,
  "properties": {
    "Argument": {
      "type": "object",
      "required": [
        "TABLE_NAME",
        "TABLE_TYPE"
      ],
      "additionalProperties": false,
      "properties": {
        "COMPONENTS": {
          "type": "array",
          "description": "ComponentsofResultTable",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "uniqueItems": true,
          "x-ui": {
            "label": "Components"
          }
        },
        "DISP_OPT": {
          "type": "string",
          "description": "DisplacementOption",
          "enum": [
            "Accumulative",
            "Current",
            "Real"
          ],
          "default": "Current",
          "x-ui": {
            "component": "Dropdown",
            "label": "Displacement Option"
          }
        },
        "EXPORT_PATH": {
          "type": "string",
          "description": "ResultTableSavePath",
          "minLength": 1,
          "x-ui": {
            "label": "Export Path"
          }
        },
        "LOAD_CASE_NAMES": {
          "type": "array",
          "description": "StaticLoad/LoadCombinationName&Type",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "minItems": 1,
          "uniqueItems": true,
          "x-ui": {
            "label": "Load Cases"
          }
        },
        "NODE_ELEMS": {
          "type": "object",
          "description": "Node/ElementNo.Input",
          "additionalProperties": false,
          "properties": {
            "KEYS": {
              "type": "array",
              "description": "SpecifyEachID",
              "items": {
                "type": "integer",
                "minimum": 1
              },
              "minItems": 1,
              "uniqueItems": true,
              "x-ui": {
                "label": "IDs"
              }
            },
            "STRUCTURE_GROUP_NAME": {
              "type": "string",
              "description": "SpecifyStructureGroupName",
              "minLength": 1,
              "x-ui": {
                "label": "Structure Group Name"
              }
            },
            "TO": {
              "type": "string",
              "description": "SpecifyIDRange",
              "pattern": "^\\s*\\d+\\s*[-~:]\\s*\\d+\\s*$",
              "x-ui": {
                "label": "ID Range (e.g., 1-10)"
              }
            }
          },
          "x-ui": {
            "label": "Targets"
          }
        },
        "OPT_CS": {
          "type": "boolean",
          "description": "Activation-ConstructionStageStep",
          "default": false,
          "x-ui": {
            "label": "Construction Stage Option"
          }
        },
        "STAGE_STEP": {
          "type": "array",
          "description": "StageStepName",
          "items": {
            "type": "string",
            "minLength": 1
          },
          "minItems": 1,
          "uniqueItems": true,
          "x-ui": {
            "label": "Stage Steps"
          }
        },
        "STYLES": {
          "type": "object",
          "description": "ResponseNumberFormat",
          "additionalProperties": false,
          "properties": {
            "FORMAT": {
              "type": "string",
              "description": "Numberformat",
              "enum": [
                "Default",
                "Fixed",
                "Scientific",
                "General"
              ],
              "default": "Default",
              "x-ui": {
                "component": "Dropdown",
                "label": "Format"
              }
            },
            "PLACE": {
              "type": "integer",
              "description": "Digitplace",
              "default": 3,
              "minimum": 0,
              "maximum": 15,
              "x-ui": {
                "label": "Decimal Places"
              }
            }
          },
          "x-ui": {
            "label": "Number Format"
          }
        },
        "TABLE_NAME": {
          "type": "string",
          "description": "ResponseTableTitle",
          "minLength": 1,
          "x-ui": {
            "label": "Table Name"
          }
        },
        "TABLE_TYPE": {
          "type": "string",
          "description": "ResultTableType",
          "enum": [
            "DISPLACEMENTG",
            "DISPLACEMENTL"
          ],
          "x-ui": {
            "component": "Dropdown",
            "label": "Table Type"
          }
        },
        "UNIT": {
          "type": "object",
          "description": "ResponseUnitSetting",
          "additionalProperties": false,
          "properties": {
            "DIST": {
              "type": "string",
              "description": "Length",
              "minLength": 1
            },
            "FORCE": {
              "type": "string",
              "description": "Force",
              "minLength": 1
            },
            "HEAT": {
              "type": "string",
              "description": "Heat",
              "minLength": 1
            },
            "TEMP": {
              "type": "string",
              "description": "Temperature",
              "minLength": 1
            }
          },
          "x-ui": {
            "label": "Units"
          }
        }
      },
      "x-ui": {
        "label": "Argument"
      }
    }
  }
}
