{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "PSC Design Code 설정 다이얼로그 - AASHTO-LRFD20 기반 프리스트레스 콘크리트 설계 파라미터",
  "properties": {
    "designCode": {
      "default": "AASHTO-LRFD20",
      "description": "적용할 설계 기준 코드",
      "enum": [
        "AASHTO-LRFD20"
      ],
      "title": "Design Code",
      "type": "string"
    },
    "inputParameters": {
      "description": "입력 파라미터",
      "properties": {
        "constructionType": {
          "default": "NON_SEGMENTAL",
          "description": "시공 형식",
          "enum": [
            "SEGMENTAL",
            "NON_SEGMENTAL"
          ],
          "title": "Construction Type",
          "type": "string",
          "x-enum-labels": {
            "NON_SEGMENTAL": "Non-Segmental",
            "SEGMENTAL": "Segmental"
          }
        },
        "corrosiveCondition": {
          "default": "SEVERE",
          "description": "부식 조건",
          "enum": [
            "SEVERE",
            "MODERATE_MILD"
          ],
          "title": "Corrosive Condition",
          "type": "string",
          "x-enum-labels": {
            "MODERATE_MILD": "Moderate/Mild",
            "SEVERE": "Severe"
          }
        },
        "exposureFactorForCrackWidth": {
          "description": "균열폭 노출계수",
          "properties": {
            "classType": {
              "default": "CLASS_I",
              "description": "노출 등급",
              "enum": [
                "CLASS_I",
                "CLASS_II",
                "USER"
              ],
              "title": "Class Type",
              "type": "string",
              "x-enum-labels": {
                "CLASS_I": "Class I (1.0)",
                "CLASS_II": "Class II (0.75)",
                "USER": "User"
              }
            },
            "userValue": {
              "default": 1,
              "description": "사용자 정의 노출계수 값",
              "title": "User Value",
              "type": "number",
              "x-optional-when": {
                "field": "classType",
                "operator": "notEquals",
                "value": "USER"
              }
            }
          },
          "required": [
            "classType"
          ],
          "title": "Exposure Factor for Crack Width",
          "type": "object"
        },
        "flexuralStrength": {
          "default": "CODE",
          "description": "휨 강도 계산 방법",
          "enum": [
            "CODE",
            "STRAIN_COMPATIBILITY"
          ],
          "title": "Flexural Strength",
          "type": "string",
          "x-enum-labels": {
            "CODE": "Code",
            "STRAIN_COMPATIBILITY": "Strain Compatibility"
          }
        },
        "tendonType": {
          "default": "LOW_RELAXATION_TENDONS",
          "description": "텐던 종류",
          "enum": [
            "LOW_RELAXATION_TENDONS",
            "STRESS_RELIEVED_TENDONS",
            "PRESTRESSING_BARS"
          ],
          "title": "Tendon Type",
          "type": "string",
          "x-enum-labels": {
            "LOW_RELAXATION_TENDONS": "Low Relaxation Tendons",
            "PRESTRESSING_BARS": "Prestressing Bars",
            "STRESS_RELIEVED_TENDONS": "Stress Relieved Tendons"
          }
        }
      },
      "required": [
        "tendonType",
        "corrosiveCondition",
        "flexuralStrength",
        "exposureFactorForCrackWidth",
        "constructionType"
      ],
      "title": "Input Parameters",
      "type": "object"
    },
    "outputParameters": {
      "description": "출력 파라미터",
      "properties": {
        "atConstructionStageServiceLoads": {
          "description": "시공 단계/사용하중 시 출력 항목",
          "properties": {
            "crackCheck": {
              "default": false,
              "description": "균열 검토",
              "title": "Crack Check",
              "type": "boolean"
            },
            "principalStressByConstructionStage": {
              "default": false,
              "description": "시공 단계별 주응력",
              "title": "Principal Stress by Construction Stage",
              "type": "boolean"
            },
            "principalStressByServiceLoadCombinationMaxTorsion": {
              "default": false,
              "description": "사용하중 조합별 주응력 (최대 비틀림)",
              "title": "Principal Stress by Service Load Combination (Max Torsion)",
              "type": "boolean"
            },
            "principalStressByServiceLoadCombinationsMaxShear": {
              "default": false,
              "description": "사용하중 조합별 주응력 (최대 전단)",
              "title": "Principal Stress by Service Load Combinations (Max Shear)",
              "type": "boolean"
            },
            "stressByConstructionStage": {
              "default": false,
              "description": "시공 단계별 응력",
              "title": "Stress by Construction Stage",
              "type": "boolean"
            },
            "stressByServiceLoadCombinations": {
              "default": false,
              "description": "사용하중 조합별 응력",
              "title": "Stress by Service Load Combinations",
              "type": "boolean"
            },
            "stressInPrestressingTendons": {
              "default": false,
              "description": "프리스트레싱 텐던 응력",
              "title": "Stress in Prestressing Tendons",
              "type": "boolean"
            }
          },
          "title": "At Construction Stage/Service Loads",
          "type": "object"
        },
        "atFactoredLoads": {
          "description": "계수하중 시 출력 항목",
          "properties": {
            "combinedShearAndTorsionCheck": {
              "default": false,
              "description": "전단 및 비틀림 복합 검토",
              "title": "Combined Shear and Torsion Check",
              "type": "boolean"
            },
            "flexuralStrengthCheck": {
              "default": false,
              "description": "휨 강도 검토",
              "title": "Flexural Strength Check",
              "type": "boolean"
            },
            "shearStrengthCheck": {
              "default": false,
              "description": "전단 강도 검토",
              "title": "Shear Strength Check",
              "type": "boolean"
            }
          },
          "title": "At Factored Loads",
          "type": "object"
        }
      },
      "title": "Output Parameters",
      "type": "object"
    }
  },
  "required": [
    "designCode",
    "inputParameters"
  ],
  "title": "PSC Design Code",
  "type": "object"
}
