{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "DCODE": {
      "description": "Design Code",
      "type": "string"
    },
    "bCOMBINED_CHECK": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Combined Shear and Torsion Check"
      }
    },
    "bCRACK_CHECK": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Crack Check"
      }
    },
    "bFLEXURAL_CHECK": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Flexural Strength Check"
      }
    },
    "bPRINCIPAL_CS": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Construction Stage"
      }
    },
    "bPRINCIPAL_SHEAR": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Service Load Combinations (Max Shear)"
      }
    },
    "bPRINCIPAL_TORSION": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Principal Stress by Service Load Combination (Max Torsion)"
      }
    },
    "bSHEAR_CHECK": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Factored Loads",
        "label": "Shear Strength Check"
      }
    },
    "bSTRESS_CS": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress by Construction Stage"
      }
    },
    "bSTRESS_PT": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress in Prestressing Tendons"
      }
    },
    "bSTRESS_SL": {
      "default": true,
      "type": "boolean",
      "x-ui": {
        "group": "At Construction Stage/Service Loads",
        "label": "Stress by Service Load Combinations"
      }
    },
    "dEXPOSURE_VALUE": {
      "default": 1,
      "description": "User-defined Exposure Factor",
      "type": "number",
      "x-ui": {
        "group": "Input Parameters",
        "label": "Exposure Factor Value",
        "visibleWhen": {
          "iEXPOSURE": 2
        }
      }
    },
    "iCONSTRUCTION": {
      "default": 0,
      "description": "Construction Type",
      "enum": [
        0,
        1
      ],
      "type": "integer",
      "x-ui": {
        "component": "radio",
        "group": "Input Parameters",
        "label": "Construction Type",
        "options": [
          {
            "label": "Segmental",
            "value": 0
          },
          {
            "label": "Non-Segmental",
            "value": 1
          }
        ]
      }
    },
    "iCORROSIVE": {
      "default": 0,
      "description": "Corrosive Condition",
      "enum": [
        0,
        1
      ],
      "type": "integer",
      "x-ui": {
        "component": "radio",
        "group": "Input Parameters",
        "label": "Corrosive Condition",
        "options": [
          {
            "label": "Severe",
            "value": 0
          },
          {
            "label": "Moderate/Mild",
            "value": 1
          }
        ]
      }
    },
    "iEXPOSURE": {
      "default": 2,
      "description": "Exposure Factor for Crack Width",
      "enum": [
        0,
        1,
        2
      ],
      "type": "integer",
      "x-ui": {
        "component": "radio",
        "group": "Input Parameters",
        "label": "Exposure Factor for Crack Width",
        "options": [
          {
            "label": "Class I (1.0)",
            "value": 0
          },
          {
            "label": "Class II (0.75)",
            "value": 1
          },
          {
            "label": "User",
            "value": 2
          }
        ]
      }
    },
    "iFLEXURAL": {
      "default": 0,
      "description": "Flexural Strength",
      "enum": [
        0,
        1
      ],
      "type": "integer",
      "x-ui": {
        "component": "radio",
        "group": "Input Parameters",
        "label": "Flexural Strength",
        "options": [
          {
            "label": "Code",
            "value": 0
          },
          {
            "label": "Strain Compatibility",
            "value": 1
          }
        ]
      }
    },
    "iTENDON_TYPE": {
      "default": 1,
      "description": "Tendon Type",
      "enum": [
        0,
        1,
        2
      ],
      "type": "integer",
      "x-ui": {
        "component": "radio",
        "group": "Input Parameters",
        "label": "Tendon Type",
        "options": [
          {
            "label": "Low Relaxation Tendons",
            "value": 0
          },
          {
            "label": "Stress Relieved Tendons",
            "value": 1
          },
          {
            "label": "Prestressing Bars",
            "value": 2
          }
        ]
      }
    }
  },
  "required": [
    "DCODE",
    "iTENDON_TYPE",
    "iCORROSIVE",
    "iFLEXURAL",
    "iCONSTRUCTION",
    "iEXPOSURE"
  ],
  "title": "PSC Design Code",
  "type": "object"
}
