{
  "ELEM": {
    "type": "object",
    "required": ["TYPE", "MATL", "NODE"],
    "properties": {
      "TYPE": {
        "type": "string",
        "description": "Element type",
        "enum": ["BEAM", "TRUSS", "TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS", "PLSTRN", "AXISYM", "SOLID"],
        "default": "BEAM",
        "x-ui": {
          "group": "Common Keys and Solid"
        }
      },
      "MATL": {
        "type": "integer",
        "description": "Material number",
        "x-ui": {
          "group": "Common Keys and Solid"
        }
      },
      "SECT": {
        "type": "integer",
        "description": "Section or thickness number",
        "x-ui": {
          "group": "Common Keys and Solid",
          "visibleWhen": {
            "TYPE": ["BEAM", "TRUSS", "TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS", "PLSTRN", "AXISYM"]
          }
        }
      },
      "NODE": {
        "type": "array",
        "description": "Node numbers",
        "items": {
          "type": "integer"
        },
        "minItems": 2,
        "maxItems": 8,
        "x-node-count-by-type": {
          "BEAM": 2,
          "TRUSS": 2,
          "TENSTR": 2,
          "COMPTR": 2,
          "PLATE": [3, 4],
          "WALL": 4,
          "PLSTRS": [3, 4],
          "PLSTRN": [3, 4],
          "AXISYM": [3, 4],
          "SOLID": [4, 6, 8]
        },
        "x-ui": {
          "group": "Common Keys and Solid"
        }
      },
      "ANGLE": {
        "type": "number",
        "description": "Beta angle",
        "default": 0,
        "x-ui": {
          "visibleWhen": {
            "TYPE": ["BEAM", "TRUSS", "TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS", "PLSTRN", "AXISYM"]
          }
        }
      },
      "STYPE": {
        "type": "integer",
        "description": "Element subtype",
        "x-enum-by-type": {
          "TENSTR": [1, 2, 3],
          "COMPTR": [1, 2],
          "PLATE": [1, 2, 3, 4],
          "WALL": [1, 2],
          "PLSTRS": [1, 2]
        },
        "x-enum-labels-by-type": {
          "TENSTR": {
            "1": "Truss",
            "2": "Hook",
            "3": "Cable"
          },
          "COMPTR": {
            "1": "Truss",
            "2": "Gap"
          },
          "PLATE": {
            "1": "Thick",
            "2": "Thin",
            "3": "Thick with Drilling DOF",
            "4": "Thin with Drilling DOF"
          },
          "WALL": {
            "1": "Membrane",
            "2": "Plate"
          },
          "PLSTRS": {
            "1": "with Drilling DOF Inactive",
            "2": "with Drilling DOF Active"
          }
        },
        "x-ui": {
          "group": "Common Keys and Solid",
          "visibleWhen": {
            "TYPE": ["TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS"]
          }
        }
      },
      "TENS": {
        "type": "number",
        "description": "Tension or compression value",
        "default": 0,
        "x-value-constraint": {
          "TENSTR": "Negative Value Only (Allowable Compression)",
          "COMPTR": "Positive Value Only (Allowable Tension)"
        },
        "x-ui": {
          "group": "Tension / Compression",
          "visibleWhen": {
            "TYPE": ["TENSTR", "COMPTR"]
          }
        }
      },
      "T_LIMIT": {
        "type": "number",
        "description": "Force limit value",
        "default": 0,
        "x-value-constraint": {
          "TENSTR": "Positive Value Only (Tension Limit Value)",
          "COMPTR": "Negative Value Only (Compression Limit Value)"
        },
        "x-ui": {
          "group": "Tension / Compression",
          "visibleWhen": {
            "TYPE": ["TENSTR", "COMPTR"]
          }
        }
      },
      "T_bLMT": {
        "type": "boolean",
        "description": "Use force limit flag",
        "default": false,
        "x-ui": {
          "group": "Tension / Compression",
          "visibleWhen": {
            "TYPE": ["TENSTR", "COMPTR"]
          }
        }
      },
      "NON_LEN": {
        "type": "number",
        "description": "Nonlinear length",
        "default": 0,
        "x-ui": {
          "group": "Tension / Compression",
          "visibleWhen": {
            "TYPE": ["TENSTR", "COMPTR"]
          }
        }
      },
      "CABLE": {
        "type": "integer",
        "description": "Cable type",
        "enum": [1, 2, 3],
        "x-enum-labels": {
          "1": "Pretension",
          "2": "Horizontal",
          "3": "Lu"
        },
        "x-required-when": {
          "TYPE": "TENSTR",
          "STYPE": 3
        },
        "x-ui": {
          "group": "Tension only - Cable",
          "visibleWhen": {
            "TYPE": "TENSTR",
            "STYPE": 3
          }
        }
      },
      "C_RAT": {
        "type": "number",
        "description": "Cable length ratio",
        "x-ui": {
          "group": "Tension only - Cable",
          "visibleWhen": {
            "TYPE": "TENSTR",
            "STYPE": 3
          }
        }
      },
      "WALL": {
        "type": "integer",
        "description": "Wall identifier",
        "x-required-when": {
          "TYPE": "WALL"
        },
        "x-ui": {
          "group": "Wall",
          "visibleWhen": {
            "TYPE": "WALL"
          }
        }
      },
      "W_CON": {
        "type": "integer",
        "description": "Wall orientation method",
        "enum": [0, 1, 2],
        "x-enum-labels": {
          "0": "Beta Angle",
          "1": "Ref Point",
          "2": "Ref Vector"
        },
        "x-required-when": {
          "TYPE": "WALL"
        },
        "x-ui": {
          "group": "Wall",
          "visibleWhen": {
            "TYPE": "WALL"
          }
        }
      },
      "W_TYPE": {
        "type": "integer",
        "description": "Wall type",
        "enum": [0, 1, 2],
        "default": 0,
        "x-enum-labels": {
          "0": "Plate base",
          "1": "CRB - Pin",
          "2": "CRB - Fixed"
        },
        "x-ui": {
          "group": "Wall",
          "visibleWhen": {
            "TYPE": "WALL"
          }
        }
      },
      "LCAXIS": {
        "type": "integer",
        "description": "Local axis"
      }
    },
    "allOf": [
      {
        "if": {
          "properties": {
            "TYPE": {
              "enum": ["BEAM", "TRUSS", "TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS", "PLSTRN", "AXISYM"]
            }
          }
        },
        "then": {
          "required": ["SECT"]
        }
      },
      {
        "if": {
          "properties": {
            "TYPE": {
              "enum": ["TENSTR", "COMPTR", "PLATE", "WALL", "PLSTRS"]
            }
          }
        },
        "then": {
          "required": ["STYPE"]
        }
      },
      {
        "if": {
          "properties": {
            "TYPE": {
              "const": "TENSTR"
            },
            "STYPE": {
              "const": 3
            }
          }
        },
        "then": {
          "required": ["CABLE", "NON_LEN"]
        }
      },
      {
        "if": {
          "properties": {
            "TYPE": {
              "const": "WALL"
            }
          }
        },
        "then": {
          "required": ["WALL", "W_CON"]
        }
      }
    ],
    "additionalProperties": false
  }
}
